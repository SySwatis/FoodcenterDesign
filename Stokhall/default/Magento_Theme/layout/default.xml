<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
   <!--  <head>
        <link src="https://fonts.googleapis.com/css?family=Work+Sans" src_type="url" rel="stylesheet" type="text/css" />
    </head> -->
    <body>

        <attribute name="class" value="theme-stokhall"/>

        <referenceBlock name="logo">
            <arguments>
                <argument name="logo_file" xsi:type="string">images/stokhall-logo.svg</argument>
                <argument name="logo_img_width" xsi:type="number">99</argument>
                <argument name="logo_img_height" xsi:type="number">75</argument>
            </arguments>
        </referenceBlock>

        <referenceBlock name="footer-logo">
            <arguments>
                <argument name="logo_src" xsi:type="string">images/stokhall-logo.svg</argument>
            </arguments>
        </referenceBlock>
         
    </body>
</page>